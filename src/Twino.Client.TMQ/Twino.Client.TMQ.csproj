<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <Title>Twino.Client.TMQ</Title>
        <Product>Twino.Client.TMQ</Product>
        <Description>Twino Messaging Queue Client to connect all TMQ Servers</Description>
        <PackageTags>twino,tmq,client,mq,messaging,queue</PackageTags>
        <AssemblyVersion>3.1.2</AssemblyVersion>
        <FileVersion>3.1.2</FileVersion>
        <PackageVersion>3.1.2</PackageVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>Mehmet Helvacıköylü;Emre Hızlı</Authors>
        <PackageProjectUrl>https://github.com/mhelvacikoylu/twino</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/mhelvacikoylu/twino/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/mhelvacikoylu/twino</RepositoryUrl>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
      <DocumentationFile>bin\Debug\Twino.Client.TMQ.xml</DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DocumentationFile>bin\Release\Twino.Client.TMQ.xml</DocumentationFile>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Twino.Client.Connectors" Version="3.1.2" />
    </ItemGroup>
    
    <ItemGroup>
        <ProjectReference Include="..\Twino.Protocols.TMQ\Twino.Protocols.TMQ.csproj" />
    </ItemGroup>

</Project>
